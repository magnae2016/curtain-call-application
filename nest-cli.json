      "compilerOptions": {
        "tsConfigPath": "apps/function1/tsconfig.app.json"
      }
    },
    "function2": {
      "type": "application",
      "root": "apps/function2",
      "entryFile": "main",
      "sourceRoot": "apps/function2/src",
      "compilerOptions": {
        "tsConfigPath": "apps/function2/tsconfig.app.json"
      }
    },
    "function3": {
      "type": "application",
      "root": "apps/function3",
      "entryFile": "main",
      "sourceRoot": "apps/function3/src",
      "compilerOptions": {
        "tsConfigPath": "apps/function3/tsconfig.app.json"
      }
    }
  }
}
